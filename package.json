{
  "name": "social-network",
  "version": "1.0.0",
  "description": "Creating a MERN social media website",
  "main": "server.js",
  "scripts": {
    "test": "cd client && npm test",
    "start": "concurrently \"npm run client\" \"npm run server\"",
    "start:watch": "concurrently \"npm run client\" \"npm run server:watch\"",
    "server": "cd server && node server.js",
    "server:watch": "cd server && nodemon server.js",
    "client": "cd client && npm start",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gravatar": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.18",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
